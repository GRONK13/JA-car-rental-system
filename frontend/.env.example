# JA Car Rental System - Frontend Environment Variables
# Copy this file to .env.local or .env and configure your values

# API Base URL for backend server
VITE_API_URL=http://localhost:3001

# Alternative local URL (fallback)
VITE_LOCAL=http://localhost:3001

# Environment mode
VITE_NODE_ENV=development

# Optional: Database connection (if needed in frontend)
# VITE_DB_HOST=localhost
# VITE_DB_PORT=5432

# Optional: Enable/disable debug features
VITE_DEBUG=false